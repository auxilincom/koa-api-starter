{
  "name": "koa-api-starter",
  "version": "0.4.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Auxilin",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "auxilin"
  ],
  "scripts": {
    "build": "run-s build:**",
    "build:email": "mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "prestart": "npm run build",
    "start": "node src/app.js",
    "test": "run-s test:**",
    "test:eslint": "eslint ./src",
    "test:mocha": "NODE_ENV=test mocha --timeout 20000 --recursive --exit -c -R spec 'src/**/*.spec.js'",
    "development": "NODE_ENV=development nodemon --watch src src/app.js",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate"
  },
  "dependencies": {
    "@auxilin/common-logger": "0.3.1",
    "@auxilin/email-service": "0.2.1",
    "@auxilin/node-mongo": "1.0.1",
    "app-module-path": "2.2.0",
    "bcrypt": "3.0.5",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.1",
    "kcors": "2.2.2",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "4.1.0",
    "koa-jwt": "3.5.1",
    "koa-logger": "3.2.0",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "lodash": "4.17.11",
    "nodemon": "1.18.10"
  },
  "devDependencies": {
    "@auxilin/eslint-config": "0.1.1",
    "all-contributors-cli": "6.2.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "faker": "4.1.0",
    "mjml": "4.3.1",
    "mocha": "6.0.2",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "supertest": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auxilincom/koa-api-starter.git"
  },
  "engines": {
    "node": ">= 10"
  }
}
